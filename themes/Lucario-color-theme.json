	"tokenColors": [
		{
			"settings": {
				"foreground": "#F8F8F2",
				"selectionBorder": "#10171e",
				"findHighlight": "#3F9AB7",
				"findHighlightForeground": "#FFF"
			}
		},
		{
			"scope": [
				"punctuation",
				"text",
				"support",
				"meta"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "italic",
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "bold",
			"scope": [
				"strong",
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "underline",
			"scope": [
				"underline",
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "heading",
			"scope": [
				"heading",
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5c98cd"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ca94ff"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#ca94ff"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#ca94ff"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#ff6541"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#ff6541"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#72C05D"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#72C05D"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#72C05D"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffab28"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#ff6541"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#72C05D"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		// Markdown
		{
			"name": "Bullets, lists (prose)",
			"scope": [
				"beginning.punctuation.definition.list.markdown",
    			"beginning.punctuation.definition.quote.markdown",
				"punctuation.definition.link.restructuredtext"
			],
			"settings": {
				"foreground": "#72C05D"
			}
		},
		{
			"name": "Inline code",
			"scope": [
				"markup.inline.raw",
				"markup.raw.restructuredtext"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Link text, image alt text",
			"scope": [
				"meta.link.reference.def.restructuredtext",
				"punctuation.definition.directive.restructuredtext",
				"string.other.link.description",
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#ca94ff"
			}
		},
		{
			"name": "Blockquotes",
			"scope": [
				"entity.name.directive.restructuredtext",
				"markup.quote"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f0cc04"
			}
		},
		{
			"name": "Horizontal rule",
			"scope": "meta.separator.markdown",
			"settings": {
				"foreground": "#5c98cd"
			}
		},
		{
			"name": "Code blocks",
			"scope": [
				"fenced_code.block.language",
				"markup.raw.inner.restructuredtext",
				"markup.fenced_code.block.markdown punctuation.definition.markdown"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Prose constants",
			"scope": "punctuation.definition.constant.restructuredtext",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Braces in markdown headings",
			"scope": [
				"markup.heading punctuation.definition.string.begin",
				"markup.heading punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#66D9EF"
			}
		}
